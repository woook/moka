SELECT dbo_DNATestRequest.RequestID, dbo_DNATestRequest.Disorder, dbo_DNATestRequest.TestClass, dbo_DNATestRequest.SpecimenNo, dbo_DNASpecimen.DateReceived, dbo_DNANumber.DNANo, dbo_DNASpecimen.Concentration, [Lastname] & "  " & [Firstname] AS Name, dbo_Patient.Sex, dbo_DNATestRequest.DateRequested
FROM ((dbo_DNATestRequest INNER JOIN dbo_DNASpecimen ON dbo_DNATestRequest.SpecimenNo = dbo_DNASpecimen.SpecimenNo) INNER JOIN dbo_DNANumber ON dbo_DNASpecimen.SpecimenNo = dbo_DNANumber.SpecimenNo) INNER JOIN dbo_Patient ON dbo_DNASpecimen.PatientID = dbo_Patient.PatientID
WHERE (((dbo_DNATestRequest.Disorder) like "Fragile X*") AND ((dbo_DNATestRequest.TestClass) Not Like "*PGD*" And (dbo_DNATestRequest.TestClass)<>"Diagnostic test") AND ((dbo_DNATestRequest.DateRequested)>=#4/1/2012#));
